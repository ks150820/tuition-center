// prettier-ignore
{
  "extends": "@tsconfig/react-native/tsconfig.json", /* Recommended React Native TSConfig base */
  "compilerOptions": {
    /* Visit https://aka.ms/tsconfig.json to read more about this file */
    /* Completeness */
    "skipLibCheck": true,
    "baseUrl": "./src",
    "paths": {
      "@util/*": [
        "util/*",
      ],
      "@component/*": [
        "./components/*"
      ],
      "@services/*": [
        "./services/*"
      ],
      "@hooks/*": [
        "./hooks/*"
      ],
      "@navigators/*": [
        "./navigators/*"
      ],
      "@screens/*": [
        "./screens/*"
      ],
      "@store/*": [
        "./store/*"
      ],
      "@theme/*": [
        "./theme/*"
      ],
      "@widgets/*": [
        "./widgets/*"
      ],
      "@common/*": [
        "./common/*"
      ],
      "@assets/*": [
        "./assets/*"
      ],
      "underscore": [
        "lodash"
      ],
      "@resources/*": ["./resources/*"],
    },
  },
  "typeRoots": [
    "./types"
  ]
}